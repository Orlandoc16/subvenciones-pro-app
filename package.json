{
  "name": "subvenciones-pro",
  "version": "2.0.0",
  "description": "Plataforma expandida de búsqueda de subvenciones con agregación de múltiples APIs oficiales",
  "type": "module",
  "private": true,
  "keywords": [
    "subvenciones",
    "grants",
    "spain",
    "api-aggregation",
    "snpsap",
    "bdns",
    "government-data"
  ],
  "author": {
    "name": "Tu Nombre",
    "email": "tu@email.com",
    "url": "https://tudominio.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuusuario/subvenciones-pro.git"
  },
  "homepage": "https://subvenciones-pro.tudominio.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "dev:safe": "cross-env ESLINT_NO_DEV_ERRORS=true vite --host",
    "dev:no-eslint": "vite --host --config vite.config.no-eslint.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run --reporter=verbose --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "deploy": "npm run build && ./scripts/deploy-subvenciones-expanded.sh",
    "validate-deployment": "./scripts/validate-deployment.sh",
    "monitor-apis": "./scripts/monitor-apis.sh",
    "clean-cache": "./scripts/clean-cache.sh",
    "clean-deps": "./scripts/clean-dependencies.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "@tanstack/react-query": "^5.56.2",
    "@tanstack/react-query-devtools": "^5.56.2",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.447.0",
    "clsx": "^2.1.1",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.5.2",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.3",
    "xlsx": "^0.18.5",
    "chart.js": "^4.4.4",
    "react-chartjs-2": "^5.2.0",
    "recharts": "^2.12.7",
    "framer-motion": "^11.8.0",
    "react-hook-form": "^7.53.0",
    "zod": "^3.23.8",
    "@hookform/resolvers": "^3.9.0",
    "react-hot-toast": "^2.4.1",
    "react-helmet-async": "^2.0.5",
    "use-debounce": "^10.0.3",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "react-intersection-observer": "^9.13.1",
    "react-lazyload": "^3.2.1",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0",
    "lru-cache": "^11.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^22.7.4",
    "@types/lodash-es": "^4.17.12",
    "@types/react-window": "^1.8.8",
    "@eslint/js": "^9.11.1",
    "@eslint/config-array": "^0.18.0",
    "@eslint/object-schema": "^2.1.4",
    "eslint": "^9.11.1",
    "typescript-eslint": "^8.7.0",
    "globals": "^15.9.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-import": "^2.30.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-pwa": "^0.20.5",
    "vite-bundle-analyzer": "^0.11.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^5.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.13",
    "postcss": "^8.4.47",
    "autoprefixer": "^10.4.20",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "vitest": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1",
    "jsdom": "^25.0.1",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "msw": "^2.4.9",
    "playwright": "^1.47.2",
    "@playwright/test": "^1.47.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "glob": "^11.0.0"
  },
  "overrides": {
    "inflight": "npm:lru-cache@^11.0.1",
    "sourcemap-codec": "@jridgewell/sourcemap-codec@^1.5.0",
    "@humanwhocodes/config-array": "@eslint/config-array@^0.18.0",
    "@humanwhocodes/object-schema": "@eslint/object-schema@^2.1.4",
    "rimraf": "^6.0.1",
    "glob": "^11.0.0",
    "eslint": "^9.11.1"
  },
  "engines": {
    "node": ">=20.12.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.22.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "npm@10.8.2"
}
