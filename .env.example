# Variables de entorno para SubvencionesPro

# API URLs (Producción)
VITE_API_BDNS_URL=https://www.pap.hacienda.gob.es/bdnstrans/GE/es
VITE_API_SNPSAP_URL=https://www.infosubvenciones.es/bdnstrans/es
VITE_API_DATOS_GOB_URL=https://datos.gob.es/apidata

# API Keys (si son necesarias)
VITE_API_KEY_BDNS=your_api_key_here
VITE_API_KEY_SNPSAP=your_api_key_here
VITE_API_KEY_DATOS_GOB=your_api_key_here

# Configuración de la aplicación
VITE_APP_NAME=SubvencionesPro
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Configuración de notificaciones
VITE_EMAIL_SERVICE_URL=https://api.emailservice.com
VITE_PUSH_SERVICE_URL=https://api.pushservice.com
VITE_SMS_SERVICE_URL=https://api.smsservice.com

# Analytics (opcional)
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
VITE_HOTJAR_ID=XXXXXXX

# Configuración de caché
VITE_CACHE_TIMEOUT=300000
VITE_API_TIMEOUT=30000

# Límites de la aplicación
VITE_MAX_EXPORT_ITEMS=1000
VITE_MAX_SEARCH_RESULTS=500
VITE_PAGE_SIZE=20

# Feature flags
VITE_FEATURE_ALERTS=true
VITE_FEATURE_EXPORT=true
VITE_FEATURE_DASHBOARD=true
VITE_FEATURE_ADVANCED_FILTERS=true

# URLs de servicios externos
VITE_SUPPORT_URL=https://support.subvencionespro.es
VITE_DOCS_URL=https://docs.subvencionespro.es
VITE_FEEDBACK_URL=https://feedback.subvencionespro.es

# Configuración de desarrollo
VITE_DEV_MODE=true
VITE_MOCK_DATA=true
VITE_DEBUG=false
